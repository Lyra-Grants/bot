{
  "name": "@lyra-grants/app",
  "version": "1.0.0",
  "description": "Lyra Bot!",
  "license": "MIT",
  "config": {
    "node-options": "--optimize_for_size --max_old_space_size=2560 --gc_interval=100"
  },
  "scripts": {
    "dev": "ts-node-dev --files --respawn src/app.ts",
    "clean": "rimraf ./build",
    "build": "yarn clean && npx tsc",
    "start": "node build/app/src/app.js",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:ts:fix": "yarn lint:ts --fix",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "deploycmds": "tsc && node ./deploy-commands.js",
    "deploycmdsbtc": "tsc && node ./deploy-commands-btc.js",
    "deploycmdslyra": "tsc && node ./deploy-commands-lyra.js"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@lyrafinance/lyra-js": "^0.0.26",
    "axios": "^1.3.2",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.7",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ethers": "5.6.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-schedule": "^2.1.1",
    "nullthrows": "^1.1.1",
    "react": "^18.2.0",
    "rimraf": "^4.1.2",
    "rpc-websocket-client": "^1.1.4",
    "telegraf": "^4.11.2",
    "ts-node": "^10.5.0",
    "twitter-api-v2": "^1.14.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  }
}
